{
	"name": "@ctx-core/store",
	"version": "27.0.22",
	"description": "ctx-core store",
	"keywords": [
		"ctx-core",
		"store"
	],
	"homepage": "https://github.com/ctx-core/store#readme",
	"bugs": {
		"url": "https://github.com/ctx-core/store/issues"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/ctx-core/store/issues"
	},
	"license": "Apache-2.0",
	"author": "Brian Takita",
	"type": "module",
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		},
		"./package.json": "./package.json"
	},
	"scripts": {
		"build": "npm run clean && npm run copy-mjs && npm run compile",
		"clean": "rm -f tsconfig.tsbuildinfo && rm -f src/**/*.js && rm -f src/**/*.d.ts && rm -f src/**/*.map",
		"compile": "./node_modules/.bin/tsc -b .",
		"copy-mjs": "cd dist && echo \"module.exports = (async () => import('./index.js'))()\" > index.cjs",
		"exec": "$@",
		"prepublishOnly": "npm run build"
	},
	"dependencies": {
		"@ctx-core/array": "workspace:^23.0.16",
		"@ctx-core/combinators": "workspace:^8.0.13",
		"@ctx-core/function": "workspace:^20.1.1",
		"@ctx-core/object": "workspace:^21.0.15",
		"esm": "^3.2.25",
		"svelte": "3.42.3"
	},
	"devDependencies": {
		"typescript": "^4.3.5"
	},
	"publishConfig": {
		"access": "public",
		"cache": "~/.npm"
	}
}